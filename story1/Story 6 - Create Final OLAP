CREATE TABLE [Final OLAP] (
    [SSN] int,
    [Borrower FirstName] varchar(50),
    [Borrower LastName] varchar(50),
    [Borrower Email] varchar(50),
    [Home Phone] varchar(50),
    [Cell Phone] varchar(50),
    [Marital Status] varchar(50),
    [Date of Birth] datetime,
    [City] varchar(50),
    [State] varchar(50),
    [Zip] varchar(50),
    [YearsAtThisAddress] int,
    [Sex] varchar(50),
    [Ethnicity] varchar(50),
    [Race] varchar(50),
    [MonthlyIncome] varchar(50),
    [Bonuses] varchar(50),
    [Commission] varchar(50),
    [OtherIncome] varchar(50),
    [Rent or Own] varchar(50),
    [Checking] int,
    [Savings] int,
    [RetirementFund] int,
    [MutualFund] int,
    [Property_ID] int,
    [Property Usage] varchar(50),
    [Property City] varchar(50),
    [Property State] varchar(50),
    [Property Zip] int,
    [RealEstateAgentEmail] varchar(50),
    [RealEstateAgentName] varchar(50),
    [RealEstateAgentPhone] varchar(10),
    [Loan_ID] int,
    [Purpose of Loan] varchar(50),
    [LoanAmount] int,
    [Purchase Price] int,
    [CreditCardAuthorization] varchar(50),
    [Number of Units] int,
    [Refferal] varchar(50),
    [Co-Borrower SSN] int,
    [Current Street Address] varchar(50),
    [Copy of Current Street Address] varchar(50)
)
